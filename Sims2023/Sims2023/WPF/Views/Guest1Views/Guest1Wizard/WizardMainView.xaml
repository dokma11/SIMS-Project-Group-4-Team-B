<Window x:Class="Sims2023.WPF.Views.Guest1Views.Guest1Wizard.WizardMainView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        ResizeMode="NoResize"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sims2023.WPF.Views.Guest1Views.Guest1Wizard"
        xmlns:cmd="clr-namespace:Sims2023.WPF.Commands" WindowStartupLocation="CenterScreen"
        mc:Ignorable="d"
        Title="WizardMainView" Height="600" Width="1000">
    
    <Grid>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="60"/>
                <RowDefinition/>
                <RowDefinition Height="40"/>
            </Grid.RowDefinitions>
            <Label x:Name="label" Content="Upoznavanje sa aplikacijom" Foreground="#E0942C" VerticalContentAlignment="Bottom" HorizontalContentAlignment="Center" FontSize="28" Grid.Row="0"/>
            <Grid x:Name="MainFrameGrid" Grid.Row="1" Margin="50,0,50,0" >
                <Frame x:Name="MainFrame" Margin="0,0,0,0" TabIndex="13" NavigationUIVisibility="Hidden" IsTabStop="False" UseLayoutRounding="True" Grid.Row="2"/>
            </Grid>
        </Grid>
    </Grid>
</Window>
